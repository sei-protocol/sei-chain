// Data-dependent: get latest block hash, then getBlockByHash(blockHash, true). Run evm_rpc_tests.sh when local.
>> {"jsonrpc":"2.0","id":1,"method":"eth_getBlockByNumber","params":["latest",false]}
<< {"jsonrpc":"2.0","id":1,"result":{}}
@ bind blockHash = result.hash
>> {"jsonrpc":"2.0","id":1,"method":"eth_getBlockByHash","params":["${blockHash}",true]}
<< {"jsonrpc":"2.0","id":1,"result":{}}
